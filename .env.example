# Environment Variables Example
# Copy this file to .env.local and fill in your values

# Application
VITE_APP_NAME="釣果記録アプリ"
VITE_APP_VERSION=""
VITE_APP_DESCRIPTION="個人用釣果記録PWAアプリ"

# Build Configuration
NODE_ENV=development
VITE_BUILD_DATE=""

# API Configuration
VITE_API_BASE_URL=""
VITE_API_TIMEOUT=10000

# Weather API Configuration
# Get your API key from: https://openweathermap.org/api
REACT_APP_WEATHER_API_KEY=""

# Analytics (for future use)
VITE_GOOGLE_ANALYTICS_ID=""
VITE_SENTRY_DSN=""

# Performance Monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_PERFORMANCE_SAMPLING_RATE=0.1

# PWA Configuration
VITE_PWA_ENABLED=true
VITE_PWA_CACHE_VERSION=1

# Feature Flags
VITE_FEATURE_ADVANCED_SEARCH=true
VITE_FEATURE_EXPORT_IMPORT=true
VITE_FEATURE_STATISTICS=true
VITE_FEATURE_GPS=true
VITE_FEATURE_PHOTO_UPLOAD=true

# Debug
VITE_DEBUG_MODE=false
VITE_ENABLE_CONSOLE_LOGS=true